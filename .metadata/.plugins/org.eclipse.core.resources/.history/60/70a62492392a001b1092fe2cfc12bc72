package test_p20;

public class test_p20 {

		public static void main(String[] args) 
		{
			Car car1=new Car();
			car1.num=1234;
			car1.gas=-10;
			car1.show();
		}
		static class Car
		{
			int num;
			private double gas;
			void show()
			{
				System.out.println("車號是"+this.num);
				System.out.println("汽油量是"+this.gas);
			}
		}

}
